{
  "name": "PlayPause",
  "version": "1.0.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["xr.js"]
  },
  "commands": {
    "ya": {
      "suggested_key": {
        "default": "F4"
      }
    },
    "yb": {
      "suggested_key": {
        "default": "Alt+Shift+U"
      }
    }
  },
  "permissions": [
      "*://youtube.com/*",
      "*://www.youtube.com/*"
  ]
}